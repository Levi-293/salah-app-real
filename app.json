{
  "expo": {
    "name": "Salah Guide",
    "slug": "salah",
    "version": "1.3.2",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.png",
    "scheme": "salahapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#053529",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "salahapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "ios": {
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "salahapp",
              "exp+salah"
            ]
          }
        ],
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "remote-notification",
          "location"
        ],
        "UIRequiredDeviceCapabilities": [
          "audio"
        ],
        "NSLocationWhenInUseUsageDescription": "We need your location to show accurate prayer times and qibla direction",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location to update prayer times and qibla direction as you move",
        "NSLocationAlwaysUsageDescription": "We need your location to update prayer times and qibla direction as you move"
      },
      "NSUserNotificationsUsageDescription": "This app needs to send you notifications for prayer times.",
      "supportsTablet": true,
      "bundleIdentifier": "com.aburuqayyah.salah",
      "buildNumber": "50",
      "googleServicesFile": "./ios/SalahGuide/GoogleService-Info.plist",
      "appStoreUrl": "https://apps.apple.com/us/app/salah-guide-app/id6737063241",
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#053529",
        "stayVisible": true
      }
    },
    "android": {
      "googleServicesFile": "./android/app/google-services.json",
      "permissions": [
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "SCHEDULE_EXACT_ALARM",
        "USE_EXACT_ALARM",
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.WAKE_LOCK",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.USE_EXACT_ALARM",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.png",
        "backgroundColor": "#053529"
      },
      "splash": {
        "image": "./assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#053529",
        "stayVisible": true
      },
      "package": "com.aburuqayyah.salah",
      "versionCode": 50,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.aburuqayyah.salah",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "salahapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/mp3/*"
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-notifications",
        {
          "sounds": []
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "salah",
          "organization": "salah-guide"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c577ef76-7320-4eca-afe0-e8ca3061b709"
      }
    },
    "runtimeVersion": "1.3.2",
    "updates": {
      "url": "https://u.expo.dev/c577ef76-7320-4eca-afe0-e8ca3061b709"
    },
    "owner": "aburuqayyah"
  }
}
